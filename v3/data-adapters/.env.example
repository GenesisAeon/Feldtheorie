# Phase 4 Data Adapters - Environment Configuration

# API Keys (obtain from respective services)
NASA_EARTHDATA_TOKEN=your_token_here
NOAA_API_KEY=your_key_here

# Data source endpoints
GRACE_TELLUS_API=https://grace.jpl.nasa.gov/data/get-data/
RAPID_MOCHA_API=https://www.rapid.ac.uk/rapidmoc/rapid_data/
NOAA_OISST_API=https://www.ncei.noaa.gov/data/sea-surface-temperature-optimum-interpolation/v2.1/access/avhrr/

# Cache settings
DATA_CACHE_DIR=./cache
CACHE_TTL_HOURS=24

# Logging
LOG_LEVEL=INFO
LOG_FILE=./logs/adapters.log

# Rate limiting
MAX_REQUESTS_PER_MINUTE=60
