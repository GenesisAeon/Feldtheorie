      "dataset_name",
      "domain",
      "control_parameter_R",
      "threshold_Theta",
      "steepness_beta",
      "impedance_zeta",
      "provenance",
      "units",
      "preprocessing_steps"
    ],
    "optional_fields": [
      "falsification_notes",
      "ΔAIC_linear",
      "ΔAIC_powerlaw",
      "R_squared",
      "imagery_metaphor",
      "analysis_link"
    ],
    "example": "{\n  \"dataset_name\": \"lenski_citplus\",\n  \"domain\": \"biology\",\n  \"control_parameter_R\": \"generation_number\",\n  \"threshold_Theta\": 32.77,\n  \"steepness_beta\": 5.08,\n  \"impedance_zeta\": \"ζ(R) = 1.25 - 0.35σ\",\n  \"provenance\": \"Lenski LTEE Ara-3 lineage\",\n  \"units\": {\"R\": \"thousands of generations\", \"response\": \"Cit+ frequency\"},\n  \"preprocessing_steps\": [\"log-transform\", \"normalization\"],\n  \"falsification_notes\": \"ΔAIC_linear=32.4, ΔAIC_powerlaw=28.7\",\n  \"R_squared\": 0.990,\n  \"imagery_metaphor\": \"evolutionary dawn chorus\",\n  \"analysis_link\": \"analysis/results/lenski_citplus_fit.json\"\n}\n"
